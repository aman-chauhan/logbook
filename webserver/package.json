		"@vitest/coverage-v8": "^4.0.13",
		"@vitest/ui": "^4.0.13",
		"happy-dom": "^20.0.10",
		"jsdom": "^27.2.0",
		"svelte": "^5.41.0",
		"svelte-check": "^4.3.3",
		"typescript": "^5.9.3",
		"vite": "^7.1.10",
		"vitest": "^4.0.13"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"bootstrap": "^5.3.8"
